Sub Lblfalha_Click
	If Not(mode = "startFalhaMode") Then
		mode = "startFalhaMode"
		CallSub2(Starter, "RequestFile", mode)
		Else
		mode = "stopAnyMode"
		CallSub2(Starter, "RequestFile", mode)
	End If
	MsgboxAsync("Comando " & mode & " enviado","")
End Sub

