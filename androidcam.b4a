End Sub

Sub Activity_Pause (UserClosed As Boolean)
	If UserClosed Then
		'File.Delete(File.DirInternal, "default.json")
		'File.Delete(File.DirInternal, "status.json")
		Activity.Finish
	End If
End Sub

Sub password_EnterPressed
